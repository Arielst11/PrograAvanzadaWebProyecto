@model FrontEnd.Models.TipoUsuarioViewModel

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout_System.cshtml";
}

<h1>Crear nuevo tipo de usuario:</h1>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>


            <div class="form-group">
                <label asp-for="TipoUsuario1" class="control-label"></label>
                <input asp-for="TipoUsuario1" class="form-control" />
                <span asp-validation-for="TipoUsuario1" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Crear" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Regresar a la lista</a>
</div>

