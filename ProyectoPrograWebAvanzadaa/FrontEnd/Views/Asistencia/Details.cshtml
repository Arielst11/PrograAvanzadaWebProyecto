@model FrontEnd.Models.AsistenciaViewModel

@{
    ViewData["Title"] = "Detalles";
    Layout = "~/Views/Shared/_Layout_System.cshtml";
}

<h1>Detalles de la asistencia seleccionada:</h1>
<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdAsistencia)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdAsistencia)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.AsistenciaUsuario)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.AsistenciaUsuario)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.PorcentajeAsistencia)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.PorcentajeAsistencia)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdUsuario)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdUsuario)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Editar", "Edit", new { id = Model.IdUsuario }) |
    <a asp-action="Index">Regresar a la lista</a>
</div>
